{
  "name": "feed",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"nodemon\"",
    "start:dev": "nodemon",
    "lint": "biome ci src",
    "lint:fix": "biome check --write src",
    "allint": "npm run lint:fix && npm run lint",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "start": "npm run prisma:push && node build/index.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "postinstall": "prisma generate",
    "seed": "prisma db seed"
  },
  "prisma": {
    "seed:users": "ts-node prisma/seed/seed.users.ts",
    "seed:feeds": "ts-node prisma/seeds/seed.feeds.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@prisma/client": "^6.16.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "fastify": "^5.6.0",
    "fastify-plugin": "^5.0.1",
    "json-schema-to-ts": "^3.1.1",
    "node-cron": "^4.2.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
